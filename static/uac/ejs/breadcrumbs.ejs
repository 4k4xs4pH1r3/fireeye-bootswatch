<ol class="breadcrumb">
    <% _.each (items, function(item, index, list) { %>
    <li>
        <% var is_last = index == list.length - 1 %>
        <% var is_linkable = ((!is_last) && item.value) %>
        <% if (is_linkable) { %>
        <a <%  is_last ? print('class="active"') : print('') %> data-value="<%= item.value %>">
        <% } %>
            <%= item.title %>
        <% if (is_linkable) { %>
        </a>
        <% } %>
    </li>
    <% }) %>
</ol>