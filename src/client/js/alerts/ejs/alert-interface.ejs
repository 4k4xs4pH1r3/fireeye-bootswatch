<% if (alert.interface) { %>
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-globe"></i> Interface
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-sm-2">
                        Name
                    </div>
                    <div class="col-sm-10">
                        <%= alert.interface.interface %>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-2">
                        Label
                    </div>
                    <div class="col-sm-10">
                        <%= alert.interface.label %>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-2">
                        Mode
                    </div>
                    <div class="col-sm-10">
                        <%= alert.interface.mode %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<% } %>