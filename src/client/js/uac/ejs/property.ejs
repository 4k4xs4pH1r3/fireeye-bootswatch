<% if (data.properties) { %>
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default">
            <% if (data.title) { %>
            <div class="panel-heading">
                <%= data.title %>
            </div>
            <% } %>
            <div class="panel-body">
                <% if (data.properties) { %>
                    <% _.each(_.keys(data.properties), function(label) { %>
                    <div class="row">
                        <div class="col-sm-2">
                            <label><%= label %></label>
                        </div>
                        <div class="col-sm-10">
                            <%= data.properties[label] %>
                        </div>
                    </div>
                    <% }) %>
                <% } %>
            </div>
        </div>
    </div>
</div>
<% } %>